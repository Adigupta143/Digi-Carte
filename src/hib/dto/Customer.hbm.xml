<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
    
    <class name="Customer" table="customertable">
      <id name="custId" column="custid"/> 
      <property name="custName" column="custname"/>
      <property name="pass" column="password"/>
      <property name="emailId" column="emailid"/>
      <property name="contact" column="contact"/>
      <property name="address" column="address"/>
       <property name="purpose" column="purpose"/>
    </class>
      
    </hibernate-mapping>